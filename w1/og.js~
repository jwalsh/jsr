console.log("og.js");

$(function() {
  $.getJSON(
    "https://graph.facebook.com/btaylor?access_token=2227470867|2.6m1jRauMRHwSjpKfCLLBAw__.3600.1288242000-1178471455|Xv2AmU2Vzj5hu_ZTtISzg_1-12w&callback=?",
    function(response) {
      console.log(response);
      $("#fullName").html("Name: " + response.first_name + " " + response.last_name);
      $("#location").html("Location: " + response.hometown.name);
      for (var w in response.work) {
        $("<li>")
          .html(response.work[w].employer.name)
          .bind(
            "click",
            // function () {
            //   alert(response.work[w].employer.name);
            // }
            function (work) {
              return function() {
                alert(work.employer.name);
              }
            }(response.work[w])
          )
          .appendTo("#work");
      }
    } 
  )

})